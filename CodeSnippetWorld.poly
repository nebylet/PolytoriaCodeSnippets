||            if v==hitPart then-- checks if v is the leaving target\n\z
||                table.remove(touchingParts,i)\n\z
||                break-- exits loop since we removed the target that stopped touching\n\z
||            end\n\z
||        end\n\z
||    end\n\z
||end)\n\z
||while true do\n\z
