              <string name="Source">local npc = script.Parent["NPC"]:Clone()
npc.Name = "og"
local pos = npc.Position
npc.Position = Vector3.New(0,0,999)
npc.Anchored = true

script.Parent["NPC"].Died:Connect(function()
	script.Parent["NPC"]["Counter"].Text = script.Parent["NPC"].Health
	script.Parent["NPC"]:Delete(2)
	script.Parent["NPC"].Name = "Dead"
	
	local new = npc:Clone()
	new.Name = "NPC"
	new.Anchored = false
	new.Position = pos
end)

while true do

	if script.Parent["NPC"] ~=nil then
		script.Parent["NPC"]["Counter"].Text = script.Parent["NPC"].Health
	end
	wait(0.1)
end</string>
